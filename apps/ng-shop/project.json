{
  "projectType": "application",
  "sourceRoot": "apps/ng-shop/src",
  "prefix": "nera",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/ng-shop",
        "index": "apps/ng-shop/src/index.html",
        "main": "apps/ng-shop/src/main.ts",
        "polyfills": "apps/ng-shop/src/polyfills.ts",
        "tsConfig": "apps/ng-shop/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": ["apps/ng-shop/src/favicon.ico", "apps/ng-shop/src/assets"],
        "styles": [
          "apps/ng-shop/src/styles.scss",
          "node_modules/quill/dist/quill.core.css",
          "node_modules/quill/dist/quill.snow.css"
        ],
        "scripts": ["node_modules/quill/dist/quill.js"]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/ng-shop/src/environments/environment.ts",
              "with": "apps/ng-shop/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "ng-shop:build:production"
        },
        "development": {
          "browserTarget": "ng-shop:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "ng-shop:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/ng-shop/**/*.ts", "apps/ng-shop/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/ng-shop"],
      "options": {
        "jestConfig": "apps/ng-shop/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
